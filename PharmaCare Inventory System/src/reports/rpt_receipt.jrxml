	<queryString language="SQL">
		<![CDATA[SELECT
    inv.id AS invoice_id,
    inv.net_total,
    inv.paid_amount,
    inv.balance,
    inv.sale_date,
    item.quantity,
    item.unit_price,
    med.name AS medicine_name
FROM
    invoice AS inv
INNER JOIN
    invoice_item AS item ON inv.id = item.invoice_id
INNER JOIN
    medicine AS med ON item.medicine_id = med.id
WHERE
    inv.id = $P{P_INVOICE_ID}]]>
